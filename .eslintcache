TokenResult = await user.getIdTokenResult();\r\n      dispatch({\r\n        type: 'LOGGED_IN_USER',\r\n        payload: {\r\n          name: user.name,\r\n          email: user.email,\r\n          token: idTokenResult.token,\r\n        },\r\n      });\r\n      props.hide();\r\n    } catch (err) {\r\n      console.log(err);\r\n    }\r\n  };\r\n  return (\r\n    <div className={classes.login}>\r\n      <div className={classes.header}>\r\n        <h1>Log In</h1>\r\n      </div>\r\n      <div className={classes.form}>\r\n        <form onSubmit={submitHandler}>\r\n          <input\r\n            id='email-login'\r\n            className={classes.email}\r\n            type='email'\r\n            value={email}\r\n            onChange={(e) => setEmail(e.target.value)}\r\n            placeholder='Email'\r\n            autoFocus\r\n          />\r\n          <input\r\n            id='password-login'\r\n            className={classes.password}\r\n            type='password'\r\n            value={password}\r\n            onChange={(e) => setPassword(e.target.value)}\r\n            placeholder='Password'\r\n          />\r\n          <span className={classes.forgotPassword} onClick={() => props.setShowAuth(3)}>\r\n            Forgot Password?\r\n          </span>\r\n          {emailError ? <Error message={emailError} /> : null}\r\n          {passwordError ? <Error message={passwordError} /> : null}\r\n          <button type='submit'>Log in</button>\r\n        </form>\r\n        <h6>or</h6>\r\n        <button onClick={googleLoginHandler}>Continue with Google</button>\r\n      </div>\r\n      {/* {showForgotPassword ? <ForgotPassword /> : null} */}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","C:\\Users\\Anurag\\Desktop\\FlashKart\\client\\src\\reducer\\userReducer.js",[],"C:\\Users\\Anurag\\Desktop\\FlashKart\\client\\src\\reducer\\rootReducer.js",[],"C:\\Users\\Anurag\\Desktop\\FlashKart\\client\\src\\functions\\login.js",[],"C:\\Users\\Anurag\\Desktop\\FlashKart\